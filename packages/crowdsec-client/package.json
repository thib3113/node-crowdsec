{
  "name": "crowdsec-client",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/types/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "ts-types": " tsc --emitDeclarationOnly --outDir lib/types",
    "build": "npm run generate-pkg &&  rimraf lib && node ./esbuild.js --analyze && npm run ts-types",
    "debug": "npm run build && node -r source-map-support/register ./lib/esm/debug.mjs",
    "generate-pkg": "node -p \"`export const pkg = { name: '${process.env.npm_package_name}', version: '${process.env.npm_package_version}' };`\" > src/pkg.ts",
    "generate-api": "node --loader ts-node/esm generator.ts",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:ci": "jest --ci",
    "test:ci:coverage": "jest --ci --coverage",
    "lint": "eslint -f unix \"src/**\"",
    "lint:fix": "npm lint -- --fix",
    "ci:eslint": "npm run lint -- -f json -o ./coverage/eslint-report.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/debug": "^4.1.8",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.19",
    "jest": "^29.5.0",
    "jest-sonar": "^0.2.16",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "swagger-typescript-api": "^12.0.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1"
  }
}
